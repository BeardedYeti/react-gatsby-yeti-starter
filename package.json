    "glamorous": "^4.10.0",
    "react": "^16.0.0",
    "react-disqus-comments": "^1.1.1",
    "react-dom": "^16.0.0",
    "react-fa": "^5.0.0",
    "react-helmet": "^5.2.0",
    "react-load-script": "0.0.6",
    "react-share": "^2.0.0",
    "sharp": "^0.19.0",
    "typography-theme-kirkham": "^0.16.3"
